{
  "name": "nestjs-boilerplate",
  "private": true,
  "description": "",
  "author": "Dmitry Kologrivko",
  "license": "ISC",
  "scripts": {
    "postinstall": "npm run bootstrap && npm run build",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap --hoist",
    "publish:release": "npm run build && lerna publish",
    "publish:alpha:preminor": "npm run build && lerna publish --canary preminor --preid alpha --dist-tag=alpha",
    "publish:alpha:premajor": "npm run build && lerna publish --canary premajor --preid alpha --dist-tag=alpha",
    "update:packages": "lernaupdate",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:cov": "jest --config jest.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.json --runInBand",
    "test:e2e": "jest --config jest-e2e.json --runInBand"
  },
  "devDependencies": {
    "@nestjs/common": "7.6.18",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "7.6.18",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/testing": "7.6.18",
    "@nestjs/typeorm": "7.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "jest": "^26.4.2",
    "jest-mock-extended": "^1.0.10",
    "lerna": "^3.22.1",
    "lerna-update-wizard": "^0.17.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typeorm": "^0.2.26",
    "typescript": "4.3.5"
  }
}
