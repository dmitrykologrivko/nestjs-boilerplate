{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Dmitry Kologrivko",
  "license": "ISC",
  "types": "dist",
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@usefultools/monads": "^3.0.3",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.5",
    "prompt-sync": "^4.2.0",
    "sqlite3": "^4.2.0",
    "create-ts-index": "^1.13.3"
  },
  "devDependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "0.0.7",
    "@nestjs/core": "^6.11.11",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.47",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "rxjs": "^6.5.5",
    "reflect-metadata": "^0.1.13",
    "jest": "^26.1.0",
    "jest-mock-extended": "^1.0.9",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "0.0.7",
    "@nestjs/core": "^6.11.11",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "rxjs": "^6.5.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitrykologrivko/nest-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/dmitrykologrivko/nest-boilerplate/issues"
  },
  "homepage": "https://github.com/dmitrykologrivko/nest-boilerplate#readme"
}
